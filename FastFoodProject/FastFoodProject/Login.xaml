<Window x:Class="FastFoodProject.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FastFoodProject"
        mc:Ignorable="d"
        Title="Login" Height="300" Width="300">
    <Grid>
        <Label Content="Login" HorizontalAlignment="Left" Margin="90,10,0,0" VerticalAlignment="Top" Width="124" FontSize="20" FontFamily="Rockwell"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="47,96,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="212"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="47,166,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="212"/>
        <Label Content="UserName" HorizontalAlignment="Left" Margin="24,65,0,0" VerticalAlignment="Top"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="24,135,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.132,-0.347"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="90,218,0,0" VerticalAlignment="Top" Width="107"/>
    </Grid>
</Window>
